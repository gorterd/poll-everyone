<% if logged_in? %>
  <script id="boostrap-script" type='text/javascript'>
    window.currentUser = <%= render('api/users/user.json.jbuilder', user: current_user).html_safe %>
  </script>
<% else %>
  <script id="boostrap-script" type='text/javascript'>
    window.currentParticipantId = <%= current_participant.id %>
  </script>
<% end %>


